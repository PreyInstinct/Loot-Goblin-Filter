//Runes Apperance
ItemDisplay[RUNE<18]: %ORANGE%%RUNENAME% %GRAY%[%WHITE%%RUNENUM%%GRAY%]{%NAME%}%CONTINUE%
ItemDisplay[RUNE~18-19]: %ORANGE%%RUNENAME% Rune %GRAY%[%BLUE%%RUNENUM%%GRAY%]{%NAME%}%CONTINUE%
ItemDisplay[RUNE~20-25]: %ORANGE%%RUNENAME% Rune %GRAY%[%YELLOW%%RUNENUM%%GRAY%]{%NAME%}%CONTINUE%
ItemDisplay[RUNE>25]: %ORANGE%%RUNENAME% Rune %GRAY%[%GOLD%%RUNENUM%%GRAY%]{%NAME%}%CONTINUE%

// Rune Notifications and Highlighting
ItemDisplay[${field} (RUNE<18)]: %PX-62%%TIER-3%%NAME%{%NAME%}%CONTINUE%
